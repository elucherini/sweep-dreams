# Fastlane environment variables (auto-loaded by fastlane)
# Copy this to .env and fill in your values

# App Store Connect API Key
ASC_KEY_ID=your_key_id
ASC_ISSUER_ID=your_issuer_id
# Base64-encode your .p8 file: base64 -i AuthKey_XXXXX.p8
ASC_KEY_CONTENT=base64_encoded_p8_content

# Match (code signing)
MATCH_GIT_URL=git@github.com:your-org/ios-certificates.git

# App configuration
API_URL_PROD=https://your-api.workers.dev
WEB_PUSH_CERTIFICATE_PAIR=your_vapid_public_key
